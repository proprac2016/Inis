

<html class="no-js" lang="en"><!--<![endif]-->


        <div id="content">

          <div class="form-content">

            <div class="row-fluid">
            
              <div class="container">
                <table class="table table-hover">
                  <thead>
                    <tr>
                      <th>Asistencia</th>
                      <th>Tipo</th>
                      <th colspan="3"></th>
                    </tr>
                  </thead>
                  <tbody>
                    <% @entrevista.each do |entrevistum| %>
                      <tr>
                        <td><%= entrevistum.asistencia_valor %></td>
